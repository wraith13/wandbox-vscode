{
    "name": "wandbox-vscode",
    "displayName": "wandbox-vscode",
    "description": "wandbox invoker for VSCode",
    "version": "0.0.1",
    "publisher": "wraith13",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openWandboxWeb",
        "onCommand:extension.getWandboxList",
        "onCommand:extension.showWandboxItem",
        "onCommand:extension.showWandboxListJson",
        "onCommand:extension.setWandboxFileOptions",
        "onCommand:extension.resetWandboxFileOptions",
        "onCommand:extension.invokeWandbox",
        "onCommand:extension.shareWandbox"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.openWandboxWeb",
            "title": "Open Web Site",
            "category": "Wandbox"
        },{
            "command": "extension.getWandboxList",
            "title": "List Compilers",
            "category": "Wandbox"
        },{
            "command": "extension.showWandboxItem",
            "title": "Show Compiler Info",
            "category": "Wandbox"
        },{
            "command": "extension.showWandboxListJson",
            "title": "Show Raw JSON",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileOptions",
            "title": "Set Complier",
            "category": "Wandbox"
        },{
            "command": "extension.resetWandboxFileOptions",
            "title": "Reset Options",
            "category": "Wandbox"
        },{
            "command": "extension.invokeWandbox",
            "title": "Run",
            "category": "Wandbox"
        },{
            "command": "extension.shareWandbox",
            "title": "Share",
            "category": "Wandbox"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "request": "^2.79.0"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}