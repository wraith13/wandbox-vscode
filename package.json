{
    "name": "wandbox-vscode",
    "displayName": "wandbox-vscode",
    "description": "wandbox invoker for VSCode",
    "version": "0.0.1",
    "publisher": "wraith13",
    "engines": {
        "vscode": "^1.5.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/wandbox-vscode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/wandbox-vscode.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openWandboxWeb",
        "onCommand:extension.getWandboxList",
        "onCommand:extension.showWandboxItem",
        "onCommand:extension.showWandboxListJson",
        "onCommand:extension.setWandboxFileCompiler",
        "onCommand:extension.setWandboxFileAdditionals",
        "onCommand:extension.setWandboxFileStdIn",
        "onCommand:extension.setWandboxFileOptions",
        "onCommand:extension.setWandboxFileStdIn",
        "onCommand:extension.setWandboxFileCompilerOptionRaw",
        "onCommand:extension.setWandboxFileRuntimeOptionRaw",
        "onCommand:extension.setWandboxFileSettingJson",
        "onCommand:extension.resetWandboxFileSettings",
        "onCommand:extension.invokeWandbox",
        "onCommand:extension.shareWandbox"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "title": "Wandbox configuration",
            "properties": {
                "wandbox.languageCompilerMapping": {
                    "type": "object",
                    "default": {
                        //  Windows Bat
                        "bat": null,
                        //  Clojure
                        "clojure": null,
                        //  Coffeescript
                        "coffeescript": "coffee-script-head",
                        //  C
                        "c": "clang-3.3-c",
                        //  C++
                        "cpp": "clang-head",
                        //  C#
                        "csharp": "mcs-head",
                        //  CSS
                        "css": null,
                        //  Diff
                        "diff": null,
                        //  Dockerfile
                        "dockerfile": null,
                        //  F#
                        "fsharp": null,
                        //  Git
                        "git-commit": null,
                        "git-rebase" :null,
                        //  Go
                        "go": null,
                        //  Groovy
                        "groovy": "groovy-2.2.1",
                        //  Handlebars
                        "handlebars": null,
                        //  HTML
                        "html": null,
                        //  Ini
                        "ini": null,
                        //  Java
                        "java": "java8-openjdk",
                        //  JavaScript
                        "javascript": "node-head",
                        //  JSON
                        "json": null,
                        //  Less
                        "less": null,
                        //  Lua
                        "lua": "lua-5.3.0",
                        //  Makefile
                        "makefile": null,
                        //  Markdown
                        "markdown": null,
                        //  Objective-C
                        "objective-c": null,
                        //  Perl
                        "perl": "perl-head",
                        "perl6": null,
                        //  PHP
                        "php": "php-head",
                        //  Powershell
                        "powershell": null,
                        //  Pug
                        "jade": null,
                        //  Python
                        "python": "python-head",
                        //  R
                        "r": null,
                        //  Razor (cshtml)
                        "razor": null,
                        //  Ruby
                        "ruby": "ruby-head",
                        //  Rust
                        "rust": "rust-head",
                        //  Sass
                        "scss": null, // (syntax using curly brackets)
                        "sass": null, // (indented syntax)
                        //  ShaderLab
                        "shaderlab": null,
                        //  Shell Script (Bash)
                        "shellscript": "bash",
                        //  SQL
                        "sql": "sqlite-head",
                        //  Swift
                        "swift": "swift-2.2",
                        //  TypeScript
                        "typescript": null,
                        //  Visual Basic
                        "vb": null,
                        //  XML
                        "xml": null,
                        //  XSL
                        "xsl": null,
                        //  YAML
                        "yaml": null
                    }
                },
                "wandbox.extensionCompilerMapping": {
                    "type": "object",
                    "default": {
                        "cpp": "clang-head",
                        "cxx": "clang-head",
                        "c": "clang-3.3-c",
                        "d": "dmd-head",
                        "rill": "rill-head",
                        "hs": "ghc-head",
                        "pl": "perl-head",
                        "py": "python-head",
                        "rb": "ruby-head",
                        "php": "php-head",
                        "erl": "erlang-head",
                        "ex": "elixir-head",
                        "exs": "elixir-head",
                        "js": "node-head",
                        "coffee": "coffee-script-head",
                        "sql": "sqlite-head",
                        "scala": "scala-2.12.x",
                        "lua": "lua-5.3.0",
                        "rs": "rust-head",
                        "vim": "vim-7.4.1714",
                        "swift": "swift-2.2",
                        "sh": "bash",
                        "lazy":"lazyk",
                        "lisp":"clisp-2.49.0",
                        "pas":"fpc-2.6.2",
                        "java": "java8-openjdk",
                        "groovy": "groovy-2.2.1",
                        "gvy": "groovy-2.2.1"
                    }
                }
            }
        },
        "commands": [{
            "command": "extension.openWandboxWeb",
            "title": "Open Web Site",
            "category": "Wandbox"
        },{
            "command": "extension.getWandboxList",
            "title": "List Compilers",
            "category": "Wandbox"
        },{
            "command": "extension.showWandboxItem",
            "title": "Show Compiler Info",
            "category": "Wandbox"
        },{
            "command": "extension.showWandboxListJson",
            "title": "Show Raw JSON",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileCompiler",
            "title": "Set Compiler",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileAdditionals",
            "title": "Set Additional Files",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileStdIn",
            "title": "Set StdIn",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileOptions",
            "title": "Set Options",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileCompilerOptionRaw",
            "title": "Set Compiler Option Raw",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileRuntimeOptionRaw",
            "title": "Set Runtime Option Raw",
            "category": "Wandbox"
        },{
            "command": "extension.setWandboxFileSettingJson",
            "title": "Set Settings JSON",
            "category": "Wandbox"
        },{
            "command": "extension.resetWandboxFileSettings",
            "title": "Reset Settings",
            "category": "Wandbox"
        },{
            "command": "extension.invokeWandbox",
            "title": "Run",
            "category": "Wandbox"
        },{
            "command": "extension.shareWandbox",
            "title": "Share",
            "category": "Wandbox"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "request": "^2.79.0"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}