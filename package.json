{
    "name": "wandbox-vscode",
    "displayName": "wandbox-vscode",
    "description": "wandbox invoker for VSCode",
    "version": "0.0.1",
    "publisher": "wraith13",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openWandboxWeb",
        "onCommand:extension.getWandboxList",
        "onCommand:extension.showWandboxListJson",
        "onCommand:extension.invokeWandbox"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.openWandboxWeb",
            "title": "Wandbox Web"
        },{
            "command": "extension.getWandboxList",
            "title": "Wandbox List"
        },{
            "command": "extension.showWandboxListJson",
            "title": "Wandbox List Raw"
        },{
            "command": "extension.invokeWandbox",
            "title": "Wandbox"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}